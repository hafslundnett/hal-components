{
  "name": "hal-playground",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 4201",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "build-hal": "ng build hal-components",
    "build-hal-s": "ng build hal-components && yarn build-hal-schematics",
    "build-hal-schematics": "cd projects/hal-components && yarn build",
    "watch-hal": "ng build hal-components --watch",
    "test-hal": "ng test hal-components",
    "commit": "git cz",
    "lint-ci": "ng lint",
    "test-ci": "npm run test -- --browsers=CiChrome --watch=false",
    "build-prod": "ng build hal-components && ng build --prod",
    "ci": "npm run lint-ci && npm run build-prod && npm run test-ci",
    "cypress": "cypress open",
    "test-install-schema": "npm link dist/hal-components && ng add @hafslundnett/hal-components"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.1.0",
    "@angular/cdk": "^8.2.3",
    "@angular/common": "^8.1.0",
    "@angular/compiler": "^8.1.0",
    "@angular/core": "^8.1.0",
    "@angular/forms": "^8.1.0",
    "@angular/material": "^8.2.3",
    "@angular/platform-browser": "^8.1.0",
    "@angular/platform-browser-dynamic": "^8.1.0",
    "@angular/router": "^8.1.0",
    "@hafslundnett/hdd-style": "^3.0.1",
    "date-fns": "^2.8.0",
    "prismjs": "^1.17.1",
    "rxjs": "~6.5.3",
    "tslib": "^1.9.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.17",
    "@angular-devkit/build-ng-packagr": "~0.803.17",
    "@angular/cli": "~8.3.17",
    "@angular/compiler-cli": "~8.2.13",
    "@angular/language-service": "~8.2.14",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~12.12.16",
    "codelyzer": "^5.0.0",
    "commitizen": "^4.0.3",
    "cypress": "^3.7.0",
    "cz-conventional-changelog": "^3.0.2",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.4.1",
    "karma-coverage-istanbul-reporter": "~2.1.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "ng-packagr": "^5.1.0",
    "ts-node": "~8.5.4",
    "tsickle": "^0.37.1",
    "tslint": "~5.20.1",
    "typescript": "~3.5.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
